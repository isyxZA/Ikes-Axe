                sprite_index = spr_BiPlane_Bomb_PwrUp_lt;
                x = global.rocket_x - 8;
                y = global.rocket_y + 40;
            }  
        }
    }
}
    else {
        global.biplane_joined = false;
        global.rocket_move = false;
    }
