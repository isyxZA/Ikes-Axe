          <argument>
            <kind>1</kind>
            <string>var move = 0;
var push;

move -= max(keyboard_check_pressed(vk_up), keyboard_check_pressed(ord("W")),0,);
move += max(keyboard_check_pressed(vk_down), keyboard_check_pressed(ord("S")),0,);

// wrap the cursor around instead of going off screen, allows for increase or decrease of menu items
if (move != 0){
    audio_play_sound(snd_Menu_Cue, 1, false);
    menu_pos += move;
    if (menu_pos &lt; 0) menu_pos = array_length_1d(menu) - 1;
    if (menu_pos &gt; array_length_1d(menu) - 1) menu_pos = 0;
}

push = max(keyboard_check_released(vk_enter), keyboard_check_released(vk_shift), keyboard_check_released(vk_space), 0);

if (push = 1) {
    global.transition = true;
    alarm[1] = 80;
}

</string>
          </argument>
        </arguments>
      </action>
